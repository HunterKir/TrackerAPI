package listener;

import java.util.TimerTask;

import data.TrackerDAO;
import data.TrackerDAOImpl;

public class UpdateDB extends TimerTask {
	private int userId;
	private int presId;
	private int buttonId;
	private TrackerDAO dao;
	
	public UpdateDB(int user, int preset, int button) {
		this.userId = user;
		this.presId = preset;
		this.buttonId = button;
		this.dao = new TrackerDAOImpl();
	}
	
	@Override
	public void run() {
		
	}

}
